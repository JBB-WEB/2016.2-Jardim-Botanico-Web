<body id="aplication-body">
  <br>
  <br>


  <div class="container">
  	<div class="row">
      <div class="col-md-6 col-md-offset-3">
      	<h1>Perfil de <%= current_user.name %></h1>
       <p>
        <b> Email: </b>
        <%= current_user.email %>
      </p>
      <p>
        <b> CPF: </b>
        <%= current_user.cpf %>
      </p>
      <p>
        <b> Telefone: </b>
        <%= current_user.phone %>
      </p>
      <p>
        <b> Gênero: </b>
        <%= current_user.gender %>
      </p>
      <p>
        <b> Data de Nascimento: </b>
        <%= current_user.birth %>
      </p>
      <ul>
        <li><%= link_to "Editar", user_edit_path(current_user), class: "btn btn-primary" %></li>
      </ul>
      <%= link_to "Excluir minha conta", current_user, method: :delete, data: { confirm: "Tem certeza que deseja excluir sua conta?" }, class: "btn btn-warning destroy-btn"%>
      </div>
    </div>
  </div>
</body>
