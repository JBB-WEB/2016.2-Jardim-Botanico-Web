<body id="page-top">
    <br><br>
    <br>
    <br>
    <br>
    <div class="col-md-6 col-md-offset-3">
    	<%= month_calendar events: @events do |date, events| %>
    	  <%= date %>
    	  <% events.each do |event| %>
    	    <div>
    	      <%= event.jbb_space_requested %>
    	    </div>
    	  <% end %>
    	<% end %>
		<br>
		<br>
        <br>
        <%= week_calendar events: @events do |date, events| %>
    	  <%= date %>
    	  <% events.each do |event| %>
    	    <div>
    	      <%= event.jbb_space_requested %>
    	    </div>
    	  <% end %>
    	<% end %>
</body>
